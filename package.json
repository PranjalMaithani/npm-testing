{
  "name": "@pranjalm/metal-name",
  "version": "1.0.0",
  "description": "returns a metal name",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PranjalMaithani/npm-testing.git"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --project tsconfig.build.json",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --project tsconfig.build.json",
    "lint": "eslint \"{**/*,*}.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "keywords": [
    "metal",
    "random"
  ],
  "author": "Pranjal Maithani",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">= 18.2.0"
  },
  "dependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "jest-canvas-mock": "^2.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
}